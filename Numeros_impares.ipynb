        "i=20\n",
        "\n",
        "numero=int(input('Ingrese el numero : '))\n",
        "\n",
        "for i in range(0, 20):\n",
        "  impar=numero*i+1\n",
        "  H.append(impar)\n",
        " \n",
        "print(H)\n",
        "\n",
        "for J in range(0, 10):\n",
        "  var=H[i]\n",
        "  H [i]=H[J]\n",
        "  H [J]=var\n",
        "  i=i-1\n",
        "\n",
        "print(H)"
